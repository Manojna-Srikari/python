n=int(input())
k=(n+1)//2
for i in range (0,k):
    for j in range (0,i+1):
        print("*",end=" ")
    print(end="\n")
for i in range (k,n):
    for j in range (n,i,-1):
        print("*",end=" ")
    print(end="\n")    
